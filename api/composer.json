{
  "name": "bubeha/resume-builder-api",
  "type": "project",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "php-di/php-di": "^7.0",
    "slim/psr7": "^1.6",
    "slim/slim": "4.*",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.22",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^5.13"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Alexandr Bubeshko",
      "email": "a.bubeshko@gmail.com"
    }
  ],
  "scripts": {
    "php-cs-fixer": "php-cs-fixer --config=./.php-cs-fixer.php --allow-risky=yes",
    "psalm": "vendor/bin/psalm --config=psalm.xml"
  }
}
